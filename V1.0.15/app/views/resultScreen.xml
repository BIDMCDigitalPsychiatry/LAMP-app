<Alloy>
	<Window id="resultScreen">
		<View id='headerContainer' class="headerContainer">
			<Require id = 'headerView' src ="innerHeader" title="Results"></Require>
		</View>
		<View id="contentView">
			<ListView  id="surveyList">
				<Templates>
					<ItemTemplate id="lstTemplate" name="surveyListTemplate">
						<View id="listOuterView" bindId="listOuterView" onClick="onMenuClick">
							<View id="gameNameView" onClick="onMenuClick">>
								<Label id="gameNameLabel" bindId="gameNameLabel"></Label>
							</View>
						</View>
					</ItemTemplate>
				</Templates>
				<ListSection id="lstSection"></ListSection>
			</ListView>
			<Label id="supportLabel"text="Copyright BIDMC Â© 2018. All rights reserved."></Label>
		</View>
	</Window>
</Alloy>